<!-- ==== sideprojects Section Start ==== -->
<section class="resume-section fix" id="sideprojects">
  <div class="container pt100 pb90">
    <div class="section-title mb100">
        <h2>Side projects</h2>
    </div>
    <div class="row mt40">
        <!-- Resume Item -->
        <div class="resume-js col-md-offset-2 col-md-9">
        {{#each hbsObject.sideprojects.projects}}
          {{#if this.show}}
          <div class="resume-item row">
            <div class="col-md-6 col-sm-6 resume-left wow fadeInUp" data-wow-delay="0.15s" style="visibility: hidden; animation-delay: 0.15s; animation-name: none;">
              <div>
                <a href={{this.startDate}} target="_blank"><img style="height: 50px; width: 50px" src="{{this.logo}}">
                  <h2 class="capitalize">{{this.company}}</h2>
                  <p><a href={{this.startDate}} target="_blank">Visit Project: {{this.title}}</a></p>
                  <p class="capitalize"><a href={{this.city}} target="_blank">Visit Github Repo</a></p>
                </a>
              </div>
            </div>
            <div class="col-md-6 col-sm-6 resume-right">
              <h2 class="capitalize">{{this.title}}</h2>
              <p>{{this.desc.copy}}<br><br></p>
              <p>{{this.responsibilities.copy}}<br><br></p>
              <ul>
                {{#each responsibilities.list}}
                  <li class="list-unstyled margin-bottom-10">{{this}}</li>
                {{/each}}
              </ul>
            </div>
          </div>
          {{/if}}
        {{/each}}
        </div>
    </div>
  </div>
</section>
<!-- ==== sideprojects Section End ==== -->